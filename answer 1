def count_substring(string, sub_string):
    ssiz=len(string)
    sub=len(sub_string)
    if 1<=ssiz<= 200:
        co=0
        j=0
        for i in range(ssiz):
            check = string[j:j+sub]
            if check == sub_string:
                co+=1
                j+=1
            else:
                j+=1
    return co

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)
